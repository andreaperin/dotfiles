{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "plain",
          "foreground": "cyan",
          "template": "<#ffffff>in</> {{ .Path }} ",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#ff94df",
          "template": "<#ffffff>on</> {{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": " <#ff94df><b>\ue0a0 </b></>",
            "fetch_stash_count": true
          }
        },
        {
          "type": "julia",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#d84054",
          "template": "  {{ .Full }} "
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#e9e8f3",
          "background": "#29137a",
          "template": " \uE235 {{ .Full }} {{ if .Venv }}{{ .Venv }}{{ end }}",
          "properties": {
            "fetch_virtual_env": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "lightGreen",
          "template": "❯"
        }
      ],
      "newline": true
    }
  ]
}
